{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "functions": {
    "api/test-db.ts": {
      "runtime": "nodejs18.x"
    },
    "api/debug-db.ts": {
      "runtime": "nodejs18.x"
    },
    "api/migrate.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/test-db",
      "destination": "/api/test-db.ts"
    },
    {
      "source": "/api/debug-db",
      "destination": "/api/debug-db.ts"
    },
    {
      "source": "/api/migrate",
      "destination": "/api/migrate.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}